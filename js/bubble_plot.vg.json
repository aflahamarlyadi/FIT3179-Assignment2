{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 400,
    "data": {
        "url": "https://raw.githubusercontent.com/aflahamarlyadi/FIT3179-Assignment2/main/data/cleaned-world-data-2023.csv"
    },
    "mark": "circle",
    "encoding": {
        "x": {
            "field": "Physicians per thousand",
            "type": "quantitative",
            "axis": {"title": "Physicians per thousand"}
        },
        "y": {
            "field": "Life expectancy",
            "type": "quantitative",
            "axis": {"title": "Life expectancy"}
        },
        "size": {
            "field": "Population",
            "type": "quantitative",
            "legend": {"title": "Population"}
        },
        "color": {
            "field": "Country",
            "type": "nominal",
            "legend": {"title": "Country"}
        },
        "tooltip": [
            {"field": "Country", "type": "nominal", "title": "Country"}
        ]
    }
  }
  