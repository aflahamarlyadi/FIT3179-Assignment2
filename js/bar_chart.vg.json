{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 400,
    "data": {
        "url": "https://raw.githubusercontent.com/aflahamarlyadi/FIT3179-Assignment2/main/data/cleaned-world-data-2023.csv"
    },
    "mark": {
        "type": "bar"
    },
    "encoding": {
        "x": {
            "field": "Population",
            "type": "quantitative",
            "axis": {"title": "Population"}
        },
        "y": {
            "field": "Country",
            "type": "nominal",
            "axis": {"title": "Country"}
        },
        "tooltip": [
            {"field": "Country", "type": "nominal", "title": "Country"},
            {"field": "Population", "type": "quantitative", "title": "Population"}
        ]
    }
}